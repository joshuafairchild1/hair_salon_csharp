@{
    Layout="Shared/_Layout.cshtml";
}

<h3>New stylist</h3>
<form action="/" method="post">
  <div class="form-group">
    <label for="stylist-name">Name:</label>
    <input type="text" name="stylist-name" class="form-control">
  </div>
  <div class="form-group">
    <label for="stylist-telephone">Name:</label>
    <input type="text" name="stylist-telephone" class="form-control">
  </div>
  <button type="submit" class="btn btn-primary">Add new stylist</button>
</form>
